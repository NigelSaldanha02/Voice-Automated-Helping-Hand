  62          // function for giving a delay of ms milliseconds
  63          void delay_ms(unsigned int ms)
  64          {
  65   1       unsigned int i,j;
  66   1       
  67   1       for(i=0;i<ms;i++)
  68   1       for(j=0;j<55;j++);
  69   1      }
